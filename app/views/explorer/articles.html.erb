<div class="container-fluid">
  <% if "articles".eql?(action_name) %>
    <h3><%= t 'articles.label.articles' %></h3>
  <% elsif "articles_in_sandbox".eql?(action_name) %>
    <h3><%= t 'articles.label.sandbox' %></h3>
  <% end %>

  <div class="container-fluid">
      <%= render partial: 'articles/article_list', locals: {articles: @articles} %>
  </div>
</div>