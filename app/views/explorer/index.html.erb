<% page_title = t 'root_page' %>
<%= title (page_title) %>

<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9">
      <% if !@articles.empty? %>
        <%= render partial: 'articles/article_list', locals: {articles: @articles} %>
      <% end %>
    </div>
    <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
      <%= render partial: 'photos' %>
    </div>
  </div>
</div>